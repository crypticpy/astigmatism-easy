    { "scope": ["support.type.primitive.ts", "support.type.primitive.tsx", "support.type.builtin.ts", "support.type.builtin.tsx"], "settings": { "foreground": "#FABD2F", "fontStyle": "italic" }}, // TS primitives italic yellow
